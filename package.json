{
  "name": "homebridge-daikin-local",
  "version": "2019.26.1",
  "description": "daikin plugin for homebridge: https://github.com/nfarina/homebridge",
  "license": "MIT",
  "keywords": [
    "homebridge-plugin",
    "daikin"
  ],
  "engines": {
    "node": ">=8.15.1",
    "homebridge": ">=0.4.46"
  },
  "author": "Christian Brandlehner",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/cbrandlehner/homebridge-daikin-local.git"
  },
  "dependencies": {
    "request": "^2.88.0"
  },
  "bugs": {
    "url": "https://github.com/cbrandlehner/homebridge-daikin-local/issues"
  },
  "homepage": "https://github.com/cbrandlehner/homebridge-daikin-local#readme",
  "main": "index.js",
  "scripts": {
    "test": "xo"
  },
  "devDependencies": {
    "xo": "^0.24.0"
  },
  "xo": {
    "space": false,
    "rules": {
      "camelcase": "off",
      "no-mixed-spaces-and-tabs": "warn",
      "no-useless-escape": "off",
      "indent": "off",
      "quotes": "warn",
      "capitalized-comments": "off",
      "no-var": "warn",
      "prefer-destructuring": "off",
      "prefer-arrow-callback": "off",
      "object-shorthand": ["off", "always", { "ignoreConstructors": true }],
      "quote-props": "error",
      "no-unused-vars": "warn",
      "curly": [
        "error",
        "multi-or-nest",
        "consistent"
      ],
      "no-use-before-define": [
        "error",
        {
          "classes": false
        }
      ]
    }
  }
}
